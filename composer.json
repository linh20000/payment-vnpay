{
    "name": "ommani/payment-vnpay",
    "type": "library",
    "description": "Thư viện hổ trợ tích hợp cổng thanh toán VNPay.",
    "keywords": [
        "ommani",
        "vnpay",
        "payment",
        "vietnam-payment",
        "payment-gateway",
        "vietnam-payment-gateway"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\VNPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\VNPay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
